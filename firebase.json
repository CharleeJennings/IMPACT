{
	"hosting": {
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"function": "impact"
			}
		],
		"headers": [
			{
				"source": "**/*.@(jpg|jpeg|gif|png|js)",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "max-age=7200"
					}
				]
			}
		]
	}
}
